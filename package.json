    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components-react-native": "^5.2.1",
    "babel-jest": "^29.2.1",
    "babel-loader": "^8.3.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-dom": "18.2.0",
    "react-native-storybook-loader": "^2.0.5",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
